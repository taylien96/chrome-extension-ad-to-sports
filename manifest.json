{
    
    "manifest_version": 2,
    "name": "Ads to Sports",
    "version": "1.0",
  
    
    
    
    "description": "A plain text description",
    "icons": { "16": "Icon.webp",
    "48": "Icon.webp",
    "128": "Icon.webp"

    },
  
    
    "author": "Taylor Alan and Gahl Peled",
    
    "content_scripts": [
        {
        "matches" : ["https://www.google.com/*"],
        "js" : ["./main.js"]
        }
    ],
    "options_ui": {
        "page" : "options.html",
        "open_in_tab" : false
    },
    "browser_action": {
        "default_popup" : "options.html"
    }
    
    
  }